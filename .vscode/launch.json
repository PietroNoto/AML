    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "train",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": ["--output-dir", "prova_vec_env_udr_4",
                    "--timesteps","400_000",
                    "--lr-scheduling","constant",
                    "--lr","0.0003",
                    "--use-udr","only",
                    "--checkpoint","prova_vec_env_udr_3/checkpoints/source_600000_steps.zip"]
        },
        {
            "name": "train_vision",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": ["--output-dir", "prova_vec_env_vision",
                    "--timesteps","1_000_000",
                    "--lr-scheduling","constant",
                    "--lr","0.0003",
                    "--use-udr","no",
                    "--use-vision", "True",
                    ]
        }
    ]
